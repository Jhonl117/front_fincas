
        <!-- Container Fluid-->
<div class="container-fluid" id="container-wrapper">
    <div id="listarFincas"></div>
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800 font-weight-bold">
            Gestión de Fincas
        </h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Inicio</a></li>
            <li class="breadcrumb-item"><a href="/listarFincas">Fincas</a></li>
            <li class="breadcrumb-item active" aria-current="page">
                Listar Fincas
            </li>
        </ol>
    </div>

    <!-- Row -->
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card mb-4">
                <div class="card-body">
                    <div class="d-flex justify-content-start mb-4">
                        <a class="btn btn-primary btn-sm mr-2 font-weight-bold" href="/registrarFinca">
                            <i class="fas fa-user icono mr-3 "></i>Registrar Finca</a>
                        <button id="btnGenerar" class="btn btn-secondary ml-4 btn-sm font-weight-bold">
                            <i class="fas fa-download mr-3"></i>Generar Pdf</button>
                    </div>
                    <div class="table-responsive pt-3">
                        <table class="table table-bordered rounded text-center" id="dataTable">
                            <thead class="thead-light">
                                <tr>
                                    <th>ID</th>
                                    <th>Nombre</th>
                                    <th>Area</th>
                                    <th>Valor</th>
                                    <th>Cultivos</th>
                                    <th>Estado</th>
                                    <th>Acciones</th>
                                </tr>
                            </thead>
                            <tbody>
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- Modal Modificar -->
    <div class="modal fade" id="UpdateModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabelLogout"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12 grid-margin stretch-card">
                            <div class="card fondo-form">
                                <div class="card-header fondo-form">
                                    <h4 class="card-title">Modificar Datos Finca</h4>
                                    <hr>
                                </div>
                                <div class="card-body">
                                    <div class="basic-form">
                                        <form id="formModificar">

                                            <div class="form-row">
                                                <input type="text" hidden id="txtID">
                                                <div class="form-group col-md-6 mb-4">
                                                    <label>Nombre</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="fas fa-user"></i></span>
                                                        </div>
                                                        <input type="text" id="txtNombre" class="form-control" placeholder="Nombre de la Finca">
                                                        <div class="invalid-feedback" id="txtNombreError"></div>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-6 mb-4">
                                                    <label>Área</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="fas fa-user"></i></span>
                                                        </div>
                                                        <input type="text" id="txtArea" class="form-control" placeholder="Área">
                                                        <div class="invalid-feedback" id="txtAreaError"></div>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-6 mb-4">
                                                    <label>Valor</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="fas fa-envelope-open"></i></span>
                                                        </div>
                                                        <input type="text" id="txtValor" class="form-control" placeholder="Escribir Valor">
                                                        <div class="invalid-feedback" id="txtValorError"></div>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-6 mb-4">
                                                    <label>Cultivos</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="fas fa-user"></i></span>
                                                        </div>
                                                        <input type="text" id="txtCultivos" class="form-control" placeholder="Cultivos">
                                                        <div class="invalid-feedback" id="txtCultivosError"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer justify-content-center">
                    <div class="mt-2">
                        <button type="submit" class="btn-sm btn-success mx-2" id="btnMdGuardar">
                            <i class="fas fa-check-square mr-2"></i>Guardar
                        </button>
                        <button type="submit" class="btn-sm btn-danger mx-2" data-dismiss="modal">
                            <i class="fas fa-times-circle mr-2" ></i>Cancelar
                        </button>
                        <button type="" class="btn-sm btn-warning mx-2" id="btnMdReset">
                            <i class="fas fa-exchange-alt mr-2"></i>Restablecer
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal Ver Datos -->
    <div class="modal fade" id="ShowModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabelLogout"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12 grid-margin stretch-card">
                            <div class="card fondo-form">
                                <div class="card-header fondo-form">
                                    <h4 class="card-title">Datos de la Finca</h4>
                                    <hr>
                                </div>
                                <div class="container pb-5">
                                    <div class="row flex-md-row align-items-center">
                                        <div class="col-md-4">
                                        <p class=" h4 text-center font-weight-bold mb-3">Foto</p>
                                        <img src="img/insertIMG.png" alt="Imagen" class="img-fluid">
                                        <p class=" h4 text-center font-weight-bold mt-3" id="selVerRol"></p>
                                        </div>
                                        <div class="col-mdl-6 mx-auto">
                                            <div class=" pt-3 text-center">
                                                <table class="table table-bordered rounded">
                                                    <tr class="py-2">
                                                        <th scope="row" class="bg-navbar">Nombre</th>
                                                        <td class="bg-white" id="txtVerNombre"></td>
                                                    </tr>
                                                    <tr>
                                                        <th scope="row" class="bg-navbar">Área</th>
                                                        <td class="bg-white" id="txtVerApellidos"></td>
                                                    </tr>
                                                    <tr>
                                                        <th scope="row" class="bg-navbar">Valor</th>
                                                        <td class="bg-white" id="txtVerValor"></td>
                                                    </tr>
                                                    <tr>
                                                        <th scope="row" class="bg-navbar">Cultivos</th>
                                                        <td class="bg-white" id="txtVerCultivos"></td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div> 
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>